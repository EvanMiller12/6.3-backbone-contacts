<h1>Manage Contacts</h1>
  <div class="contact-name contact-info">
    <span>Name:</span>{{contactName}}
  </div>
    <div class="contact-number contact-info">
      <span>Number:</span>{{contactNumber}}
    </div>
      <div class="contact-email contact-info">
        <span>Email:</span>{{contactEmail}}
      </div>
        <div class="contact-linkedin contact-info">
          <span>Linkedin:</span>{{contactLinkedin}}
        </div>
          <button class="btn btn-sm btn-danger delete-contact">Delete Contact</button>
